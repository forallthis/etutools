{
  "name": "etutools",
  "version": "0.1.0",
  "description": "Easy-to-use tools Chrome extension",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && shx cp manifest.json dist/ && shx cp -r assets dist/ && shx mkdir -p dist/src/tools && shx cp src/tools/tools.css dist/src/tools/",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/js-yaml": "^4.0.9",
    "@types/qrcode": "^1.5.6",
    "canvas": "^3.2.1",
    "shx": "^0.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "js-yaml": "^4.1.1",
    "jsqr": "^1.4.0",
    "qrcode": "^1.5.4"
  }
}
